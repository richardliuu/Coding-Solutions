n = int(input())

mylist = []
x_coords = []
y_coords = []

for i in range(n):
    coords = input().strip()
    mylist.append(coords)

for j in mylist:
    x = j[0:2]
    y = j[3:5]

    x_coords.append(x)
    y_coords.append(y)

bottom_left_x = int(min(x_coords)) - 1
bottom_left_y = int(min(y_coords)) - 1
bottom_right_x = int(max(x_coords)) + 1
bottom_right_y = int(max(y_coords)) + 1

print(f"{bottom_left_x},{bottom_left_y}")
print(f"{bottom_right_x},{bottom_right_y}")

