class Solution:
    def __init__(self):
        self.R = int(input())
        self.G = int(input())
        self.B = int(input())

    def main(self):
        red_price = self.R * 3 
        green_price = self.G * 4
        blue_price = self.B *5

        self.C = red_price + green_price + blue_price

        print(self.C)
    
solution = Solution()
solution.main()
