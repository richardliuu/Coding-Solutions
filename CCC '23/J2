n = int(input())

scoville = 0 

for i in range(n):
    chilli = str(input()).strip()

    if chilli == "Poblano":
        scoville += 1500
    
    if chilli == "Mirasol":
        scoville += 6000

    if chilli == "Serrano":
        scoville += 15500

    if chilli == "Cayenne":
        scoville += 40000

    if chilli == "Thai":
        scoville += 75000

    if chilli == "Habanero":
        scoville += 125000

print(scoville)
