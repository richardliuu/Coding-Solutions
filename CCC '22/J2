class Solution:
    def __init__(self):
        self.N = int(input())
        self.star_player = 0 

    def main(self):
        for i in range(self.N):
            points = int(input())
            fouls = int(input())
                
            star_points = points * 5 
            foul_points = fouls * 3

            total_player_stars = star_points - foul_points 

            if total_player_stars > 40:
                self.star_player += 1

        if self.star_player == self.N:
            print(f"{self.star_player}+")
        else:
            print(self.star_player)

solution = Solution()
solution.main()
