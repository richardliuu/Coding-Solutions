h = int(input())
M = int(input())

t = 1
touched = False

while t <= M:
    altitude = (-6 * t ** 4) + (h * t ** 3) + (2 * t ** 2) + (t)

    if altitude <= 0:
        print("The balloon first touches ground at hour: ")
        print(t)
        touched = True
        break

    t += 1

if not touched:
    print("The balloon does not touch the ground in the given time.")
